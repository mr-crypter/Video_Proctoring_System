    "react-scripts": "5.0.1",
    "socket.io-client": "^4.8.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "dev": "concurrently \"tailwindcss -i src/index.css -o src/output.css --watch\" \"react-scripts start\"",
